ShowPMPriority="false" ShowPMStatus="false" ShowPMVersion="false" ShowPackageNameStyle="0" ShowPackages="true" ShowRequirements="true" ShowShapeLegend="false" ShowShapeStereotypeIconName="true" ShowStereotypes="true" ShowTaggedValues="false" ShowTerms="true" ShowTypeCol="true" ShowUnspecifiedItems="true" ShowUseCases="true" ShowWorkflows="true" ShowingFilterPanel="0" TeamworkCreateDateTime="0" TextualAnalysisDocument="&lt;head&gt;&#13;&#10;    &lt;style type=&quot;text/css&quot;&gt;&#10;      &lt;!--&#10;        body { color: #000000; font-family: Dialog; font-size: 22px }&#10;      --&gt;&#10;    &lt;/style&gt;&#13;&#10;    &#13;&#10;  &lt;/head&gt;&#13;&#10;  &lt;body&gt;&#13;&#10;    &lt;p&gt;&#13;&#10;      Lista wymaga&amp;#324; funkcjonalnych:&#13;&#10;    &lt;/p&gt;&#13;&#10;    &lt;p&gt;&#13;&#10;      &amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;*System zawiera katalog dost&amp;#281;pnych gier z mo&amp;#380;lio&amp;#347;ci&amp;#261; dodawania &#13;&#10;      lub usuwania pozycji.&#13;&#10;    &lt;/p&gt;&#13;&#10;    &lt;p&gt;&#13;&#10;      &amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;*System pozwala wypo&amp;#380;yczy&amp;#263; gr&amp;#281; na okres 3, 7 lub 14 dni.&#13;&#10;    &lt;/p&gt;&#13;&#10;    &lt;p&gt;&#13;&#10;      &amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;*System pozwala zarezerwowa&amp;#263; kopi&amp;#281; do dw&amp;#243;ch dni przed jej &#13;&#10;      w&amp;#322;a&amp;#347;ciwym wypo&amp;#380;yczeniem.&#13;&#10;    &lt;/p&gt;&#13;&#10;    &lt;p&gt;&#13;&#10;      &amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;*System jest w stanie &amp;#347;ledzi&amp;#263; liczno&amp;#347;&amp;#263; dost&amp;#281;pnych kopii tej &#13;&#10;      samej gry w zale&amp;#380;no&amp;#347;ci od wypo&amp;#380;ycze&amp;#324;.&#13;&#10;    &lt;/p&gt;&#13;&#10;    &lt;p&gt;&#13;&#10;      &amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;*System mo&amp;#380;e przypisa&amp;#263; klienta do konkretnego wypo&amp;#380;yczonego &#13;&#10;      egzemplarza gry.&#13;&#10;    &lt;/p&gt;&#13;&#10;    &lt;p&gt;&#13;&#10;      &amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;*System pomaga pracownikowi wyda&amp;#263; odpowiedni&amp;#261; kopi&amp;#281; przy &#13;&#10;      odbiorze przez klienta.&#13;&#10;    &lt;/p&gt;&#13;&#10;    &lt;p&gt;&#13;&#10;      &amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;*System pozwala klientowi na zdalne przed&amp;#322;u&amp;#380;enie terminu &#13;&#10;      wypo&amp;#380;yczenia.&#13;&#10;    &lt;/p&gt;&#13;&#10;    &lt;p&gt;&#13;&#10;      &#13;&#10;    &lt;/p&gt;&#13;&#10;    &lt;p&gt;&#13;&#10;      Lista wymaga&amp;#324; niefunkcjonalnych:&#13;&#10;    &lt;/p&gt;&#13;&#10;    &lt;p&gt;&#13;&#10;      &amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;*Klient mo&amp;#380;e tylko przegl&amp;#261;da&amp;#263; katalog, mo&amp;#380;liwo&amp;#347;&amp;#263; modyfikacji &#13;&#10;      jest dost&amp;#281;pna tylko dla pracownik&amp;#243;w wypo&amp;#380;yczalni.&#13;&#10;    &lt;/p&gt;&#13;&#10;    &lt;p&gt;&#13;&#10;      &amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;*Wypo&amp;#380;yczanie i pre-rezerwacja wykonywane jest przez uprzednio &#13;&#10;      zarejestrowanego w systemie klienta.&#13;&#10;    &lt;/p&gt;&#13;&#10;    &lt;p&gt;&#13;&#10;      &amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;*Klient mo&amp;#380;e rezerwowa&amp;#263; i op&amp;#322;aca&amp;#263; wypo&amp;#380;yczone gry przez &#13;&#10;      internet, ale odbi&amp;#243;r jest osobisty.&#13;&#10;    &lt;/p&gt;&#13;&#10;    &lt;p&gt;&#13;&#10;      &amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;*Wypo&amp;#380;yczane produkty to kopie fizyczne - system nie obs&amp;#322;uguje &#13;&#1